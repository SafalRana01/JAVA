    a. -5 + 8 * 6
    b. (55+9) % 9
    c. 20 + -3*5 / 8
    d. 5 + 15 / 3 * 2 - 8 % 3
    
    a. -5 + 8 * 6
    
    public class Question
    {
        public static void main(String[] args)
        {
             double a = -5 + 8 * 6;
                     System.out.println(a);
        }
    }    


    
    b. (55+9) % 9
     
     public class Question
     {
          public static void main(String[] args)
